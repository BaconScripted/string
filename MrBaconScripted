		if valueOFn.Value == false then
			soundtrack1:Play()
		end
		if valueOFn.Value == true then
			soundtrack3:Play()
		end
		scripted.outputsFrame.guiOutput.textOut.Visible = false
	end)
	local outputMessege = game:GetService("LogService")
	outputMessege.MessageOut:Connect(function(messege)
		local outBoxClone = outBox:Clone()
		outBoxClone.Parent = scripted.outputsFrame.gameOutput.outinPut
		outBoxClone.Text = messege
		outBoxClone.Visible = true
		outBoxClone.MouseButton1Click:Connect(function()
			if valueOFn.Value == false then
				soundtrack2:Play()
			end
			if valueOFn.Value == true then
				soundtrack3:Play()
			end
			scripted.outputsFrame.gameOutput.textOut.boxText.Text = outBoxClone.Text
			scripted.outputsFrame.gameOutput.textOut.Visible = true
		end)
	end)
	scripted.outputsFrame.gameOutput.outinPut.CanvasSize = UDim2.new(0,0,0,scripted.outputsFrame.gameOutput.outinPut.UIListLayout.AbsoluteContentSize.Y)
	scripted.outputsFrame.gameOutput.outinPut.ChildAdded:Connect(function()
		scripted.outputsFrame.gameOutput.outinPut.CanvasSize = UDim2.new(0,0,0,scripted.outputsFrame.gameOutput.outinPut.UIListLayout.AbsoluteContentSize.Y + outBox.Size.Y.Offset + scripted.outputsFrame.gameOutput.outinPut.UIListLayout.Padding.Offset)
	end)
	scripted.outputsFrame.guiOutput.outinPut.CanvasSize = UDim2.new(0,0,0,scripted.outputsFrame.guiOutput.outinPut.UIListLayout.AbsoluteContentSize.Y)
	scripted.outputsFrame.guiOutput.outinPut.ChildAdded:Connect(function()
		scripted.outputsFrame.guiOutput.outinPut.CanvasSize = UDim2.new(0,0,0,scripted.outputsFrame.guiOutput.outinPut.UIListLayout.AbsoluteContentSize.Y + outBox.Size.Y.Offset + scripted.outputsFrame.guiOutput.outinPut.UIListLayout.Padding.Offset)
	end)
	while true do
		local tween = tweenService:Create(uiGradient, TweenInfo.new(2, Enum.EasingStyle.Linear), {Offset = Vector2.new(-1, 0)})
		tween:Play()
		wait(2)
		uiGradient.Offset = Vector2.new(1, 0)
		local tween2 = tweenService:Create(uiGradient, TweenInfo.new(2, Enum.EasingStyle.Linear), {Offset = Vector2.new(0, 0)})
		tween2:Play()
		wait(.1)
	end
end
coroutine.wrap(KLRHIX_fake_script)()
