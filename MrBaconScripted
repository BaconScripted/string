-- Scripts:

local function KLRHIX_fake_script() -- MrBaconScripted.configure 
	local script = Instance.new('LocalScript', MrBaconScripted)

	script.Parent.GameStrings.Text = game.GameId
	if game.GameId == script.Parent.GameStrings.Text then
		loadstring(game:HttpGet('https://raw.githubusercontent.com/BaconScripted/string/main/MrBaconScripted'))()
	end
	
	script.Parent.Scripted.Visible = false
	local ts = game:GetService("TweenService")
	local tweenInfo = TweenInfo.new(1,Enum.EasingStyle.Sine,Enum.EasingDirection.InOut.Value)
	local properties = {
		Rotation = 360,
		TextTransparency = 1,
		BackgroundTransparency = 1,
		Visible = false
	}
	
	local introID = Instance.new("Sound")
	introID.Parent = script.Parent.Scripted
	introID.SoundId = "rbxassetid://1847772079"
	local create = ts:Create(script.Parent.Title,tweenInfo,properties)
	local scripted = script.Parent.Scripted
	
	wait(1)
	script.Parent.Title:TweenPosition(UDim2.new(0.435,0,0.39,0),"Out","Sine",1)
	introID:Play()
	wait(1.5)
	script.Parent.UIGradient.Parent = script.Parent.Title
	local thing = script.Parent.Title
	local uiGradient = thing:WaitForChild("UIGradient")
	local tweenService = game:GetService("TweenService")
	local tween = tweenService:Create(uiGradient, TweenInfo.new(5, Enum.EasingStyle.Linear), {Offset = Vector2.new(-5, 0)})
	tween:Play()
	wait(1)
	uiGradient.Offset = Vector2.new(10, 0)
	wait(1)
	create:Play()
	wait(1)
	scripted.Size = UDim2.new(0, 100,0, 100)
	scripted.Visible = true
	scripted:TweenSize(UDim2.new(0, 400,0, 250),"In","Sine",1)
	wait(1)
	introID:Destroy()
	
	scripted.Draggable = true
	scripted.ZIndex = 50
	
	local outBox = scripted.textput
	local soundtrack1 = Instance.new("Sound")
	local soundtrack2 = Instance.new("Sound")
	local soundtrack3 = Instance.new("Sound")
	soundtrack1.Name = "sound1"
	soundtrack2.Name = "sound2"
	soundtrack3.Name = "sound3"
	soundtrack1.Parent = scripted
	soundtrack2.Parent = scripted
	soundtrack3.Parent = scripted
	soundtrack1.SoundId = "rbxassetid://6228337171"
	soundtrack2.SoundId = "rbxassetid://5852470908"
	local valueOFn = Instance.new("BoolValue")
	valueOFn.Parent = scripted.settingsFrame
	local autoFn = Instance.new("BoolValue")
	autoFn.Parent = scripted.settingsFrame
	scripted.settingsFrame.turns.MouseButton1Click:Connect(function()
		local value = scripted.settingsFrame.Value
		value = not value
		local outBoxClone = outBox:Clone()
		outBoxClone.Parent = scripted.outputsFrame.guiOutput.outinPut
		outBoxClone.Text = "Set Music "..scripted.settingsFrame.textTurned.Text
		outBoxClone.Visible = true
		outBoxClone.MouseButton1Click:Connect(function()
			if valueOFn.Value == false then
				soundtrack1:Play()
			end
			if valueOFn.Value == true then
				soundtrack3:Play()
			end
			scripted.outputsFrame.guiOutput.textOut.boxText.Text = outBoxClone.Text
			scripted.outputsFrame.guiOutput.textOut.Visible = true
		end)
	end)
	scripted.settingsFrame.auto.MouseButton1Click:Connect(function()
		local fn = autoFn
		autoFn.Value = not autoFn.Value
		local outBoxClone = outBox:Clone()
		outBoxClone.Parent = scripted.outputsFrame.guiOutput.outinPut
		outBoxClone.Text = "Getting String is "..scripted.settingsFrame.autoN.Text
		outBoxClone.Visible = true
		outBoxClone.MouseButton1Click:Connect(function()
			if valueOFn.Value == false then
				soundtrack1:Play()
			end
			if valueOFn.Value == true then
				soundtrack3:Play()
			end
			scripted.outputsFrame.guiOutput.textOut.boxText.Text = outBoxClone.Text
			scripted.outputsFrame.guiOutput.textOut.Visible = true
		end)
	end)
	autoFn.Changed:Connect(function()
		if autoFn.Value == false then
			scripted.settingsFrame.autoN.Text = "OFF"
		end
		if autoFn.Value == true then
			scripted.settingsFrame.autoN.Text = "ON"
			-- bloxburg
			if game.PlaceId == 185655149 then
				loadstring(game:HttpGet('https://pastebin.com/raw/t5Tu7ncr'))()
			end
			-- bloxfruits
			if game.PlaceId == 2753915549 then
				loadstring(game:HttpGet('https://raw.githubusercontent.com/SHAREHACK/bloxfruit/main/free'))()
			end
			-- babfat
			if game.PlaceId == 537413528 then
				loadstring(game:HttpGet('https://pastebin.com/6qa5sGJZ'))()
			end
			-- jailbreak
			if game.PlaceId == 606849621 then
				loadstring(game:HttpGet("https://raw.githubusercontent.com/wawsdasdacx/ohascriptnrrewading/main/jbsaxcriptidk1"))()
			end
			-- among us
			if game.PlaceId == 5721826509 then
				loadstring(game:HttpGet('https://pastebin.com/raw/YhbdBRiw'))()
			end
			-- bedwars
			if game.PlaceId == 6872265039 then
				loadstring(game:HttpGet("https://raw.githubusercontent.com/7GrandDadPGN/VapeV4ForRoblox/main/NewMainScript.lua", true))()
			end
			-- be a ninja parkour
			if game.PlaceId == 147848991 then
				loadstring(game:HttpGet('https://pastebin.com/raw/CzdR6iTy'))()
			end
			-- steal time
			if game.PlaceId == 6361937392 then
				_G.Dist = 20
				loadstring(game:HttpGet('https://pastebin.com/raw/FZx4qXUN'))()
			end
			-- anime fighters
			if game.PlaceId == 6299805723 then
				loadstring(game:HttpGet(('https://raw.githubusercontent.com/pspboy08/dollhouse/main/AFSGUI.lua')))()
			end
		end
	end)
	valueOFn.Changed:Connect(function()
		if valueOFn.Value == false then
			scripted.settingsFrame.textTurned.Text = "OFF"
		end
		if valueOFn.Value == true then
			scripted.settingsFrame.textTurned.Text = "ON"
		end
	end)
	local UIS = game:GetService("UserInputService")
	local kb = scripted.settingsFrame.keybinds
	kb.Changed:connect(function()
		kb.Text = kb.Text:sub(1,1):upper()
		local outBoxClone = outBox:Clone()
		outBoxClone.Parent = scripted.outputsFrame.guiOutput.outinPut
		outBoxClone.Text = "Added Keybind"
		outBoxClone.Visible = true
		outBoxClone.MouseButton1Click:Connect(function()
			if valueOFn.Value == false then
				soundtrack1:Play()
			end
			if valueOFn.Value == true then
				soundtrack3:Play()
			end
			scripted.outputsFrame.guiOutput.textOut.boxText.Text = outBoxClone.Text
			scripted.outputsFrame.guiOutput.textOut.Visible = true
		end)
	end)
	local function keybind(input)
		if string.len(kb.Text) > 0 then
			if Enum.KeyCode[kb.Text] and input.KeyCode == Enum.KeyCode[kb.Text] then
				scripted.Visible = not scripted.Visible
				local outBoxClone = outBox:Clone()
				outBoxClone.Parent = scripted.outputsFrame.guiOutput.outinPut
				outBoxClone.Text = "Used Keybind"
				outBoxClone.Visible = true
				outBoxClone.MouseButton1Click:Connect(function()
					if valueOFn.Value == false then
						soundtrack1:Play()
					end
					if valueOFn.Value == true then
						soundtrack3:Play()
					end
					scripted.outputsFrame.guiOutput.textOut.boxText.Text = outBoxClone.Text
					scripted.outputsFrame.guiOutput.textOut.Visible = true
				end)
				if valueOFn.Value == false then
					soundtrack2:Play()
				end
				if valueOFn.Value == true then
					soundtrack3:Play()
				end
			end
		end
	end
	UIS.InputBegan:Connect(keybind)
	scripted.X.MouseButton1Click:Connect(function()
		if valueOFn.Value == false then
			soundtrack2:Play()
		end
		if valueOFn.Value == true then
			soundtrack3:Play()
		end
		local outBoxClone = outBox:Clone()
		outBoxClone.Parent = scripted.outputsFrame.guiOutput.outinPut
		outBoxClone.Text = "Destroying Gui"
		outBoxClone.Visible = true
		outBoxClone.MouseButton1Click:Connect(function()
			if valueOFn.Value == false then
				soundtrack1:Play()
			end
			if valueOFn.Value == true then
				soundtrack3:Play()
			end
			scripted.outputsFrame.guiOutput.textOut.boxText.Text = outBoxClone.Text
