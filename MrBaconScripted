		if valueOFn.Value == false then
			soundtrack2:Play()
		end
		if valueOFn.Value == true then
			soundtrack3:Play()
		end
		local outBoxClone = outBox:Clone()
		outBoxClone.Parent = scripted.outputsFrame.guiOutput.outinPut
		outBoxClone.Text = "Teleporting To Be A Ninja Parkour"
		outBoxClone.Visible = true
		outBoxClone.MouseButton1Click:Connect(function()
			if valueOFn.Value == false then
				soundtrack1:Play()
			end
			if valueOFn.Value == true then
				soundtrack3:Play()
			end
			scripted.outputsFrame.guiOutput.textOut.boxText.Text = outBoxClone.Text
			scripted.outputsFrame.guiOutput.textOut.Visible = true
		end)
		scripted.teleportsFrame.ninjaparkour.BackgroundColor3 = Color3.fromRGB(231, 50, 50)
		wait(1)
		tp:Teleport(147848991, game.Players.LocalPlayer)
	end)
	scripted.teleportsFrame.bedwars.MouseButton1Click:Connect(function()
		if valueOFn.Value == false then
			soundtrack2:Play()
		end
		if valueOFn.Value == true then
			soundtrack3:Play()
