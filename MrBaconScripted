	end)
	scripted.playerFrame.info.Text = "Hello there "..game.Players.LocalPlayer.Name.."'s Thanks For Using MrBaconScripted, There's some future updates coming i guess. anyways thats all."
	scripted.settingsFrame.enter.MouseButton1Click:Connect(function()
		soundtrack3.SoundId = "rbxassetid://"..scripted.settingsFrame.soundText.Text
	end)
	scripted.outputsFrame.gameOutput.textOut.exit.MouseButton1Click:Connect(function()
		if valueOFn.Value == false then
			soundtrack1:Play()
		end
		if valueOFn.Value == true then
			soundtrack3:Play()
		end
		scripted.outputsFrame.gameOutput.textOut.Visible = false
	end)
