		-- output game

		outPut.MessageOut:Connect(function(Messege,typeOfMessege)
			local outputBoxClone = outputBox:Clone()
			outputBoxClone.Visible = true
			outputBoxClone.Text = Messege
			outputBoxClone.Parent = guiOnPlr.mainFrame.GameOutput
			outputBoxClone.MouseButton1Click:Connect(function()
				sizeFrameOutput.Strings.Text = outputBoxClone.Text
				sizeFrameOutput.titlename.Text = outputBoxClone.Parent.Name
				SecondsSound:Play()
			end)
		end)

		-- buttons with changing color

		guiOnPlr.mainFrame.Buttons.loadstrings.MouseButton1Click:Connect(function()
			guiOnPlr.mainFrame.frame3.Visible = true
			guiOnPlr.mainFrame.frame2.Visible = false
			guiOnPlr.mainFrame.frame1.Visible = false
			guiOnPlr.mainFrame.frame4.Visible = false
			guiOnPlr.mainFrame.Buttons.place.BackgroundColor3 = Color3.fromRGB(157, 157, 157)
			guiOnPlr.mainFrame.Buttons.profile.BackgroundColor3 = Color3.fromRGB(157, 157, 157)
			guiOnPlr.mainFrame.Buttons.settings.BackgroundColor3 = Color3.fromRGB(157, 157, 157)
			guiOnPlr.mainFrame.Buttons.loadstrings.BackgroundColor3 = Color3.fromRGB(100, 100,100)
			local outputBoxClone = outputBox:Clone()
			outputBoxClone.Visible = true
			outputBoxClone.Text = "Opened Loadstrings"
			outputBoxClone.Parent = guiOnPlr.mainFrame.GuiOutput
			outputBoxClone.MouseButton1Click:Connect(function()
				sizeFrameOutput.Strings.Text = outputBoxClone.Text
