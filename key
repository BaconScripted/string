		-- keybinds

		kb.Changed:connect(function()
			kb.Text = kb.Text:sub(1,1):upper()
		end)

		--Custom keybind (that's where the error is)

		local function keybind(input)
			if string.len(kb.Text) > 0 then
				if Enum.KeyCode[kb.Text] and input.KeyCode == Enum.KeyCode[kb.Text] then
					guiOnPlr.mainFrame.Visible = not guiOnPlr.mainFrame.Visible
					local outputBoxClone = outputBox:Clone()
					outputBoxClone.Visible = true
					outputBoxClone.Text = "Gui Hide/NotHide"
					outputBoxClone.Parent = guiOnPlr.mainFrame.GuiOutput
					outputBoxClone.MouseButton1Click:Connect(function()
						sizeFrameOutput.Strings.Text = outputBoxClone.Text
						sizeFrameOutput.titlename.Text = outputBoxClone.Parent.Name
						musicButton:Play()
					end)
					musicButton:Play()
				end
			end
		end

		--Triggers

		UIS.InputBegan:Connect(keybind)

		-- settings/frame2

		guiOnPlr.mainFrame.frame1.plr.Text = guiOnPlr.Parent.Parent.Name

		guiOnPlr.mainFrame.frame2.TextButton.MouseButton1Click:Connect(function()
			musicButton:Play()
			guiOnPlr:Destroy()
		end)

