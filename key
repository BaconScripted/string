	-- Sounds Instance

	local onSound = Instance.new("Sound")
	onSound.Name = "ButtonSound"
	onSound.Parent = guiOnPlr
	onSound.SoundId = "rbxassetid://6228337171"
	local musicButton = guiOnPlr.ButtonSound

	local secondSound = Instance.new("Sound")
	secondSound.Name = "SecondsSound"
	secondSound.Parent = guiOnPlr
	secondSound.SoundId = "rbxassetid://5852470908"
	local SecondsSound = guiOnPlr.SecondsSound

